{
  "name": "kite-web",
  "version": "0.3.0",
  "description": "Front-end for managing Docker containers for New England Institute of Technology students and faculty",
  "main": "src/js/main.js",
  "scripts": {
    "dev-serve": "cross-env NODE_ENV=development webpack-dev-server --inline --hot --port 8086 --mode development",
    "test": "jest tests --watchAll",
    "test-update-snapshots": "jest --updateSnapshot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neit-kite/kite-web.git"
  },
  "keywords": [
    "front-end",
    "vue",
    "vuetify",
    "vuex"
  ],
  "author": "Joseph Zurowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neit-kite/kite-web/issues"
  },
  "homepage": "https://github.com/neit-kite/kite-web#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "@sentry/browser": "^4.3.0",
    "vue": "^2.5.17",
    "vuex": "^3.0.1",
    "vue-router": "^3.0.1",
    "vuetify": "^1.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.1.5",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.5",
    "@vue/babel-preset-app": "^3.1.1",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "bundle-loader": "^0.5.6",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "extract-css-chunks-webpack-plugin": "^3.2.1",
    "file-loader": "^2.0.0",
    "html-critical-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "jest-serializer-vue": "^2.0.2",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "preload-webpack-plugin": "^3.0.0-beta.2",
    "purgecss": "^1.1.0",
    "purgecss-loader": "github:ianwalter/purgecss-loader",
    "sass-loader": "^7.1.0",
    "serverless-single-page-app-plugin": "^1.0.2",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "uglify-es": "^3.3.9",
    "uglify-js": "^3.4.9",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "vue-jest": "^3.0.0",
    "vue-loader": "^15.4.2",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.5.17",
    "vuetify-loader": "^1.0.6",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-s3-plugin": "^1.0.2"
  },
  "peerDependencies": {
    "serverless": "^1.32.0",
    "vue-cli": "^2.9.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/tests/.*|(\\.|/)(test|spec))\\.jsx?$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{vue}",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "snapshotSerializers": [
      "jest-serializer-vue"
    ]
  }
}
